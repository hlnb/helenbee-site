   ---
   layout: layouts/base.njk
   title: Categories
   eleventyNavigation:
     key: Categories
     order: 3
   ---
   <h1>Categories</h1>
   <ul>
     {% for category, posts in collections.writings | groupBy('data.category') %}
     <li>
       <h2>{{ category }}</h2>
       <ul>
         {% for post in posts %}
         <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
         {% endfor %}
       </ul>
     </li>
     {% endfor %}
   </ul>